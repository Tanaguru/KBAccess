<%@ page pageEncoding="UTF-8" %>
<ol>
    <c:forEach var="account" items="${accountStatistics}">
        <li>
            <a href="<c:url value='/account/details/${account.id}/profile.html'/>">${account.displayedName}</a>
            (${account.testcaseCount} exemple<c:if test="${account.testcaseCount > 1}">s</c:if>)
        </li>
    </c:forEach>
</ol>
