<%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core" %>
<%@ page pageEncoding="UTF-8" %>
<c:set var="result_color">
    <c:choose>
        <c:when test="${result == 'Failed'}">red</c:when>
        <c:when test="${result == 'NA'}">gray</c:when>
        <c:when test="${result == 'Success'}">green</c:when>
        <c:otherwise>black</c:otherwise>
    </c:choose>
</c:set>
<span color="${result_color}">${result}</span>