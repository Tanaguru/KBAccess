package org.opens.sdk.entity.i18n;

import org.opens.sdk.entity.Entity;

/**
 * 
 * @author jkowalczyk
 * @version 1.0.0
 * @param <E>
 */
public interface InternationalizedEntity<E> extends Internationalized, Entity {

	/**
	 * 
	 * @return the target
	 */
	E getTarget();

	/**
	 * 
	 * @param target
	 *            the target to set
	 */
	void setTarget(E target);
}
